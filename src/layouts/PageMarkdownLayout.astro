---
import BaseLayout from "./BaseLayout.astro";

const { title } = Astro.props;
---

<BaseLayout title={title}>
  <div class="container mx-auto my-4 max-w-5xl rounded-lg px-4">
    <div class="prose prose-invert max-w-none rounded-lg bg-content px-8 py-6">
      <slot />
    </div>
  </div>
</BaseLayout>
